<div class= "all events">
  <h1>Explore All Events</h1>
  <p>Explore Unique Dining Experience with New Friends</p>
</div>

<div class="event_wrapper">
  <% @events.each do |event|%>
    <div class = "event">
    <img src="/images/<%=event.image_file%>" width = "200px" height = "200px">
    <h3 align: center> Title: <%=event.title%> </h3>
    <h3> Capacity: <%=event.capacity%> </h3>
    <h3> Price: <%=event.price%> </h3>
    </div>
  <%end%>
</div>